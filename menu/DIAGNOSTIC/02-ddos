find $([ -d /var/www/httpd-logs/ ] && echo "/var/www/httpd-logs/" ; [ -d /var/log/httpd/domains/ ] && echo "/var/log/httpd/domains/"; [ -d /var/log/apache2/domains ] && echo /var/log/apache2/domains/) -iname "*.log" ! -iname "*.error.log" -mmin -2 -exec bash -c '[ $(wc -l {}|egrep -o "^\s*[0-9]+") -le 1000 ]&&exit;echo -n {}\ ; tail -n1000 {} | head -n1' \; |sort -k5|egrep --color=always '\[.*\]';date
